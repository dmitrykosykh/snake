{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "no-param-reassign": "off",
        "no-else-return": "off",
        "no-lonely-if": "off"
    },
    "parser": "babel-eslint"
}